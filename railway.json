{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "dockerfile",
    "buildCommand": "docker build -t angel ."
  },
  "deploy": {
    "startCommand": "docker run -p 8080:8080 angel",
    "healthcheckPath": "/health",
    "healthcheckTimeout": 100,
    "restartPolicyType": "never"
  }
}
